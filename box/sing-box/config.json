  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "selector",
      "tag": "Proxy",
//      "providers": [
//        "provider1",
//        "provider2",
//        "provider3"
//      ],
      "outbounds": [
        "direct"
      ]
    }
  ],
  "route": {
    "rules": [
      {
        "port": 53,
        "outbound": "dns-out"
      },
      {
        "geosite": "category-ads-all",
        "outbound": "block"
      },
      {
        "geosite": [
          "apple@cn",
          "category-games@cn"
        ],
        "outbound": "direct"
      },
      {
        "geosite": [
          "geolocation-!cn",
          "microsoft",
          "google"
        ],
        "outbound": "Proxy"
      },
      {
        "geosite": [
          "cn"
        ],
        "outbound": "direct"
      },
      {
        "geoip": [
          "cn",
          "private"
        ],
        "outbound": "direct"
      }
    ],
//    "auto_detect_interface": true,
//    "default_mark": 233,
    "find_process": true,
    "final": "Proxy"
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9998",
//      "secret": "singbox",
//      "external_ui": "dashboard",
      "store_selected": true,
      "store_fakeip": true
    }
  }
